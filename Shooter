package edu.wpi.first.wpilibj.templates;
 

import edu.wpi.first.wpilibj.Timer;
/**
 *
 * @author student
 */
public class Shooter {
    private Piston shotGate;
    private Piston shotGate2; 
    private Piston shooterPiston;
    
    public Shooter(){
    shotGate= new Piston(2);
    shotGate2= new Piston (3)
    shooterPiston = new Piston(4);
    }
    
    public void shoot(){
        shotGate.extend();
        shotGate2.extend();
        Timer.delay(0.1);
        shooterPiston.retract();
        Timer.delay(0.1);
        shotGate.retract();
        shotGate2.retract();
        shooterPiston.extend();
        /*when the method “shoot” is called, the gate piston extends and the robot waits for 0.1 seconds. 
        *This releases the shooter piston which shoots the ball. The shooter piston retracts, the robot 
        *waits for 0.1 seconds, the gate piston retracts ( the gate closes), and the shooter piston 
        *extends again. This method is written for a pre-pressurized system with a gate latch that, when 
        *released, allows the shooter piston to fully extend at a higher velocity than it would without being pre-pressurized.
        */  

    }
}
