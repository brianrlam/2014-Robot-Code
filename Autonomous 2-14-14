package edu.wpi.first.wpilibj.templates;

public class autonomous

shooter = new Shooter();
Camera = new Camera();

public void autonomous() {

if (goalsFound = true) {
    shooter.shoot();
    driveTrain.drive(1.0,0.0);
    }

else if (goalsFound = false) {
	driveTrain.drive(1.0,0.0);
	shooter.shoot();
	}
}		
